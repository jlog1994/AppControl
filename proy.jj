void Programa():
{}
{
	<PROGRAMA><IDENTIFIER><LBRACE>Principal()<RBRACE><EOF>
	
}
void Principal():
{}
	{
		<MAIN>
		<LBRACE>Sentencia()<RBRACE>
	}
void DeclaracionLocal():
{}
	{
		<INT><IDENTIFIER> <SEMICOLON> vs()
	}
	
void vs():
{}
	{
		DeclaracionLocal() |Sentencia()
	}
void Sentencia():
{}
	{
		{
			DeclaracionLocal()|SentenciaIf()
		}*
	}
	
	//sentencia if
void SentenciaIf():
{}
	{
		<IF><LPAREN> Comparsciones()<RPAREN><LBRACE><RBRACE>
	}

void Comparsciones():
{}
	{
		Valor()Operadores()Valor()
	}
	
void Valor():
{}
	{
	<IDENTIFIER>|<NUMBER>
	}
void Operadores():
{}
	{
		<MENOR>|<MAYOR>
	}
	